<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="UpdateUrl" value="https://netfoundry.jfrog.io/netfoundry/ziti-maven-snapshot/ziti-tunnel-win/amd64/windows/ziti-tunnel-win/maven-metadata.xml" />
    <!-- 
    Format for UpdateTimer is "hours:minutes:seconds" - none of the values can exceed the normal range of each tuple.
    For example this is invalid: "0:60:0" because 60 is > 59. To represent every hour you would use "1:0:0"
    
    This example shows a value that would check for updates every one hour, two minutes and three seconds
    <add key="UpdateTimer" value="1:2:3"/>
    
    This operation is should probably remain high such as "1:0:0" (every hour) or "1:0:0:0" (every day)
    -->
    <add key="UpdateTimer" value="0:1:0" />
    <!--default to look for updates once a minute-->
    <add key="Version" value="release" />
    <!--
    Format for ServiceTimer is "hours:minutes:seconds" - none of the values can exceed the normal range of each tuple.
    For example this is invalid: "0:60:0" because 60 is > 59. To represent every hour you would use "1:0:0"
    
    This example shows a value that would check for the health of the service every one hour, two minutes and three seconds
    <add key="ServiceTimer" value="1:2:3"/>
    
    This operation is fast and should probably remain low such as "0:1:0" (every minute) or "0:0:15" (every 15 seconds)
    -->
    <add key="ServiceTimer" value="0:0:5" />
    <!--default to verify the service is running every five seconds-->
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>